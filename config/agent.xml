<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Mailserver</key>
        <dict>
            <key>ip</key>
            <string>192.168.0.30</string>
            <key>services</key>
            <dict>
                <key>Exim</key>
                <dict>
                    <key>processes</key>
                    <array>
                        <string>/usr/local/sbin/exim</string>
                    </array>
                    <key>tcp-ports</key>
                    <array>
                        <integer>25</integer>
                        <integer>465</integer>
                        <integer>587</integer>
                    </array>
                </dict>
                <key>Dovecot</key>
                <dict>
                    <key>processes</key>
                    <array>
                        <string>/usr/local/sbin/dovecot</string>
                        <string>dovecot-auth</string>
                        <string>imap-login</string>
                        <string>pop3-login</string>
                    </array>
                    <key>tcp-ports</key>
                    <array>
                        <integer>110</integer>
                        <integer>143</integer>
                        <integer>993</integer>
                        <integer>995</integer>
                    </array>
                    <key>unix-sockets</key>
                    <array>
                        <string>/var/run/dovecot/auth-master</string>
                        <string>/var/run/dovecot/auth-client</string>
                    </array>
                </dict>
                <key>SpamAssassin</key>
                <dict>
                    <key>processes</key>
                    <array>
                        <string>/usr/local/bin/spamd</string>
                        <string>spamd child</string>
                    </array>
                    <key>tcp-ports</key>
                    <array>
                        <integer>783</integer>
                    </array>
                </dict>
            </dict>
        </dict>
    </dict>
</plist>